<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jonghoonlim.two_oh">

    <!--  Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/texas"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="jonghoonlim.two_oh.AddNewInventory"
            android:label="Add New Inventory" />

        <activity
            android:name="jonghoonlim.two_oh.CheckIn"
            android:label="@string/check_in"
            android:windowSoftInputMode="stateHidden"/>

        <activity
            android:name="jonghoonlim.two_oh.CheckOut"
            android:label="@string/check_out"
            android:windowSoftInputMode="stateHidden"/>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
